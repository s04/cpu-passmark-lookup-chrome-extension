{
    "manifest_version": 3,
    "name": "CPU Passmark Lookup",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "scripting"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "CPU_UserBenchmark.json",
                "cpu_passmark.json"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}